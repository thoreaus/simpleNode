
<!DOCTYPE html>
<html lang="en">
<head>
    <% include head %>
</head>
<body class="container">

    <header>
        <% include header %>
        <style>
          h1, h2, p {
            text-align: center;
            color: darkgrey;
          }
          button {
            background-color: beige;
            color: orange;
          }

        </style>
    </header>

    <main>
        <div class="jumbotron">
            <h1>Welcome to Vic Car Rentals</h1>
            <p>Look up Cars!</p>
        </div>
        <form action="/query" method="post">
            <h2>Query<h2>
            <label for="queryStr">Search:</label>
            <input id="queryStr" type="text" name="queryStr" placeholder="enter name">
            <button id="execute" type="submit">run query</button>
        </form>

        <h2>Results</h2>
        <ul>
            <% listings.forEach(function(item) { %>
                <li><%= item.name %> - <%= item.location %></li>
            <% }); %>
        </ul>
    </main>



</body>
</html>
