
<!DOCTYPE html>
<html lang="en">
<head>
    <% include head %>
</head>
<body class="container">

    <header>
        <% include header %>
    </header>

    <main>
        <div class="jumbotron">
            <h1>This is great</h1>
            <p>Welcome to templating using EJS</p>
        </div>

        <h2>Loop</h2>
        <ul>
            <% listings.forEach(function(item) { %>
                <li><%= item.make %> - <%= item.price %></li>
            <% }); %>
        </ul>
    </main>


    
</body>
</html>